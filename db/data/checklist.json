{
  "checklist": {
    "backpack_gear": {
      "required": [
        "40-60L Backpack with raincover",
        "Backpacking tent",
        "Sleeping bag",
        "Headlamp / Flashlights"
      ],
      "optional": ["Sleeping pad", "Hiking poles"]
    },
    "kitchen_tools": {
      "required": [
        "Backpacking stove",
        "Fuel",
        "Cookset (at least 1 pot)",
        "Mug/Cup",
        "Dish/Bowl",
        "Utensils",
        "Biodegradable soap",
        "Small quick-dry towel",
        "Collapsible water container"
      ]
    },
    "food": {
      "required": [
        "Energy food and drinks, trail mix",
        "Extra day's supply of food"
      ]
    },
    "water": {
      "required": [
        "Water bottle or hydration bladder",
        "Water filter or chemical treatment"
      ]
    },
    "clothes_footwear": {
      "required": [
        "Moisture-wicking undergarments",
        "Moisture-wicking t-shirt",
        "Quick-drying pants/shorts",
        "Long-sleeve shirt (for sun, bugs)",
        "Lightweight fleece or jacket",
        "Boots or shoes",
        "Socks (synthetic or wool)"
      ],
      "cold_weather": [
        "Rainwear (jacket and pants)",
        "Long underwear",
        "Insulated jacket/vest (puffy)",
        "Fleece pants",
        "Gloves or mittens",
        "Warm hat"
      ],
      "snow_weather": ["Crampons", "Ice axe"],
      "optional": ["Sandals (camp shoes)", "Gaiters", "Bandana or buff"]
    },
    "navigation": {
      "required": ["Compass", "Watch", "Map"]
    },
    "first_aid": {
      "required": [
        "Whistle",
        "Lighter/matches",
        "Emergency shelter",
        "Fire starter",
        "First-aid kit/supplies"
      ]
    },
    "hygiene": {
      "required": [
        "Toothbrush and toothpaste",
        "Sanitation towel",
        "Toilet paper/wipes in sealable bag",
        "Sunglasses",
        "Sunscreen",
        "Lip balm",
        "Sun hat"
      ],
      "optional": ["Insect repellent"]
    }
  }
}
